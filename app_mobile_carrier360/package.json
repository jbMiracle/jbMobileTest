{
  "name": "app_mobile_carrier360",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "nuke": "npm run nuke:project",
    "nuke:project": "scripts/nuke-project-dirs-interactive.js node_modules ios/Pods ios/build android/build",
    "nuke:derived-data": "scripts/nuke-derived-data-interactive.js app_mobile_carrier360",
    "nuke:all": "npm run nuke:derived-data && npm run nuke:project",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "pods": "cd ios && pod install || echo \"Dependency issues with `npm run pods`? Try `npm run pods-update`\"",
    "pods-update": "cd ios && pod update",
    "android": "ENVFILE=.env.test source ./scripts/avd-helpers.sh && LaunchAvdIfNeeded && react-native run-android",
    "ios": "ENVFILE=.env.test react-native run-ios",
    "test": "jest",
    "appium": "jest --runInBand --config e2e/Appium/jest.config.json",
    "test:watch": "jest --watch --no-coverage",
    "test:e2e:ios": "detox build -c ios.sim.debug && detox test -c ios.sim.debug",
    "build:android:prod": "cd android && ./gradlew assembleRelease && cd ..",
    "build:android:test": "export ENVFILE=.env.test && cd android && ./gradlew build && cd ..",
    "lint": "eslint src/",
    "validate": "npm run lint && npm run test"
  },
  "rnpm": {
    "assets": [
      "./src/assets/fonts"
    ]
  },
  "dependencies": {
    "Base64": "^1.0.1",
    "deep-freeze": "^0.0.1",
    "lodash": "^4.17.5",
    "native-base": "^2.4.1",
    "npm": "^6.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-native": "0.54.4",
    "react-native-animatable": "^1.2.4",
    "react-native-config": "^0.11.5",
    "react-native-fcm": "^14.1.3",
    "react-native-i18n": "^2.0.12",
    "react-native-navbar-color": "^1.0.2",
    "react-native-popup-menu": "^0.12.3",
    "react-native-side-menu": "^1.1.3",
    "react-native-swipe-gestures": "^1.0.2",
    "react-native-touch-id": "^4.0.1",
    "react-native-vector-icons": "^4.6.0",
    "react-navigation": "^1.5.9",
    "react-navigation-redux-helpers": "^1.0.3",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-persist": "^5.9.1",
    "redux-saga": "^0.16.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.44",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "22.4.3",
    "babel-preset-react-native": "4.0.0",
    "detox": "^7.3.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-babel": "^5.0.0",
    "eslint-plugin-detox": "^1.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "fs-extra": "^5.0.0",
    "i18n-js": "^3.0.3",
    "inquirer": "^5.2.0",
    "jest": "22.4.3",
    "react-dom": "^16.3.2",
    "react-test-renderer": "16.3.1",
    "reactotron-react-native": "^1.14.0",
    "reactotron-redux": "^1.13.0",
    "reactotron-redux-saga": "^1.13.0",
    "redux-saga-tester": "^1.0.379",
    "wd": "^1.6.2"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/src/themes/defaultTheme/"
    ],
    "preset": "react-native",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|native-base-shoutem-theme|@shoutem/animation|@shoutem/ui|tcomb-form-native|react-navigation|native-base/node_modules/react-native-vector-icons)"
    ],
    "setupFiles": [
      "<rootDir>/src/setupTests.js"
    ],
    "globals": {
      "__TESTING__": true
    }
  },
  "detox": {
    "test-runner": "jest",
    "runner-config": "e2e/detox/config.json",
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/app_mobile_carrier360.app",
        "build": "xcodebuild -project ios/app_mobile_carrier360.xcodeproj -scheme app_mobile_carrier360 -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone 8"
      }
    }
  }
}
